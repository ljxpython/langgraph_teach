# File RAG 模块环境模板
# 复制为 src/file_rag/.env 并填写真实配置

# ========== LLM ==========
LLM_API_KEY=sk-your-deepseek-key
LLM_API_BASE=https://api.deepseek.com/v1
LLM_MODEL=deepseek-chat
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=8192

# ========== 文档处理 ==========
MAX_FILE_SIZE_MB=50
UPLOAD_DIR=uploads
ENABLE_PDF_MULTIMODAL=true
PDF_EXTRACT_IMAGES=true
PDF_IMAGES_INNER_FORMAT=markdown-img

# ========== 图片解析模型 ==========
# Doubao Ark 网关示例；也可以替换为其他兼容 OpenAI 协议的模型
IMAGE_PARSER_API_BASE=https://ark.cn-beijing.volces.com/api/v3
IMAGE_PARSER_API_KEY=${DOUBAO_API_KEY}
IMAGE_PARSER_MODEL=doubao-seed-1-6-251015
IMAGE_PARSER_MAX_TOKENS=8192
